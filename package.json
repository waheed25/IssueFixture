{
  "name": "Wefiq",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "devmenu": "adb shell input keyevent 82",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "postinstall": "npx jetify",
    "git-hook": "npm run lint -s && npm run test -s",
    "android-dev": "adb reverse tcp:8081 tcp:8081",
    "react-devtools": "react-devtools",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "ios:bundle": "react-native bundle --entry-file index.js --platform ios --dev true --bundle-output ios/main.jsbundle --assets-dest ios",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "reload": "adb shell input keyevent 82 && adb shell input keyevent 66 && adb shell input keyevent 66",
    "debug": "adb shell input keyevent 82 && adb shell input keyevent 61 && adb shell input keyevent 66 && adb shell input keyevent 66",
    "debugger": "open \"rndebugger://set-debugger-loc?host=localhost&port=8081\""
  },
  "rnpm": {
    "assets": [
      "resources/fonts"
    ]
  },
  "dependencies": {
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/google-signin": "^3.0.3",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/netinfo": "^4.6.1",
    "@react-native-community/slider": "^2.0.8",
    "@welldone-software/why-did-you-render": "^3.3.7",
    "apisauce": "^0.14.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "format-json": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "json-api-normalizer": "^0.4.14",
    "lodash": "^4.17.11",
    "lottie-ios": "3.1.3",
    "lottie-react-native": "^3.3.2",
    "moment": "^2.24.0",
    "native-base": "2.13.7",
    "patch-package": "^6.1.2",
    "postinstall-postinstall": "^2.0.0",
    "prop-types": "^15.6.1",
    "pusher-js": "^5.0.3",
    "react-native-track-player": "1.1.8",
    "qs": "^6.6.0",
    "querystringify": "2.0.0",
    "ramda": "^0.26.1",
    "react": "16.9.0",
    "react-content-loader": "^4.3.2",
    "react-native": "0.61.2",
    "react-native-animatable": "^1.3.0",
    "react-native-config": "^0.12.0",
    "react-native-cookies": "^3.3.0",
    "react-native-device-info": "^5.3.1",
    "react-native-easy-grid": "^0.2.0",
    "react-native-exception-handler": "^2.10.8",
    "react-native-fast-image": "^6.1.1",
    "react-native-fbsdk": "^1.1.1",
    "react-native-gesture-handler": "1.5.2",
    "react-native-google-places-autocomplete": "^1.4.0",
    "react-native-image-zoom-viewer": "^2.2.24",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-largelist-v3": "^3.0.14",
    "react-native-lightbox": "^0.8.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-modal": "11.5.1",
    "react-native-modalbox": "^1.6.0",
    "react-native-no-flicker-image": "^1.0.2",
    "react-native-progress": "^3.6.0",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "2.0.0-alpha.1",
    "react-native-slowlog": "^1.0.2",
    "react-native-splash-screen": "^3.0.6",
    "react-native-svg": "^9.13.6",
    "react-native-swift": "^1.2.2",
    "react-native-swipe-gestures": "^1.0.3",
    "react-native-tab-view": "0.0.74",
    "react-native-vector-icons": "^6.6.0",
    "react-native-video": "^5.1.0-alpha2",
    "react-native-video-player": "^0.10.0",
    "react-native-webview": "^8.0.5",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "2.3.3",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-navigation-stack": "1.10.3",
    "react-navigation-tabs": "^2.6.0",
    "react-redux": "7.1.1",
    "react-timer-mixin": "^0.13.4",
    "recyclerlistview": "^1.3.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "0.7.0",
    "reselect": "^4.0.0",
    "rn-placeholder": "^2.0.0",
    "seamless-immutable": "^7.1.4",
    "styled-components": "^4.1.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.6.2",
    "@babel/runtime-corejs2": "^7.0.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-storyshots": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/channels": "^3.4.0",
    "@storybook/react-native": "^3.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^3.0.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^6.5.1",
    "ignite-andross": "^3.0.1",
    "ignite-animatable": "^1.0.2",
    "ignite-redux-persist": "^1.1.0",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.1.1",
    "jest": "23.6.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "0.56.0",
    "mockery": "^2.1.0",
    "react-dom": "16.3.0",
    "react-test-renderer": "16.9.0",
    "reactotron-react-native": "^2.1.0",
    "reactotron-redux": "^2.1.0",
    "reactotron-redux-saga": "^2.1.0",
    "snazzy": "^8.0.0",
    "standard": "10.0.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "isSwift": true
}
